<%- include('../partials/header') %>

<table id="list">
    <thead>
        <tr>
            <th>Route</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        <% tickets.forEach(function(t) { %>
            <tr>
                <td><%= t.route %></td>
                <td>$<%= t.price %></td>
            </tr>
        <% }) %>
    </tbody>
</table>

<%- include('../partials/footer') %>